{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node",
        "config": {
          "functions": {
            "controllers/*.js": { "maxDuration": 60 },
            "middleware/**/*.js": { "maxDuration": 60 }
          }
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/server.js" }
    ]
  }